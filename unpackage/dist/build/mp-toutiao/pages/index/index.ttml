<view class="content"><image style="width:750rpx;height:620rpx;" src="/static/index/header.png"></image><view class="form-phone"><input class="phone" type="text" maxlength="10" placeholder="宝贝姓名" placeholder-style="color: rgba(131, 147, 181, 1)" data-event-opts="{{[['input',[['inputName',['$event']]]]]}}" value="{{name}}" bindinput="__e"/><input class="phone" type="number" maxlength="11" placeholder="家长手机号" placeholder-style="color: rgba(131, 147, 181, 1)" data-event-opts="{{[['input',[['inputPhone',['$event']]]]]}}" value="{{phonenum}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['postForm',['$event']]]]]}}" class="get-btn" bindtap="__e">免费领取</button><text class="text">领取后会有专属写字课程顾问联系您</text></view><swiper class="swiper" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}" vertical="{{true}}" circular="{{true}}"><block tt:for="{{roll}}" tt:for-item="item" tt:for-index="index" tt:key="index"><block><swiper-item data-event-opts="{{[['touchmove',[['e0',['$event']]]]]}}" catchtouchmove="__e"><view class="swiper-item"><image class="img" src="/static/index/roll.png"></image><view class="text">{{item.name}}</view><view class="text">{{item.phonenum}}</view><view class="text">{{item.time}}</view></view></swiper-item></block></block></swiper><image style="width:750rpx;height:1378rpx;" src="/static/index/01.png"></image><image style="width:750rpx;height:744rpx;" src="/static/index/02.png"></image><image style="width:750rpx;height:1008rpx;" src="/static/index/03.png"></image><image style="width:750rpx;height:1074rpx;" src="/static/index/04.png"></image><image style="width:750rpx;height:700rpx;" src="/static/index/05.png"></image><view class="form-phone"><input class="phone" type="text" maxlength="10" placeholder="宝贝姓名" placeholder-style="color: rgba(131, 147, 181, 1)" data-event-opts="{{[['input',[['inputName',['$event']]]]]}}" value="{{name}}" bindinput="__e"/><input class="phone" type="number" maxlength="11" placeholder="家长手机号" placeholder-style="color: rgba(131, 147, 181, 1)" data-event-opts="{{[['input',[['inputPhone',['$event']]]]]}}" value="{{phonenum}}" bindinput="__e"/><button data-event-opts="{{[['tap',[['postForm',['$event']]]]]}}" class="get-btn" bindtap="__e">免费领取</button><text class="text">领取后会有专属写字课程顾问联系您</text></view><button data-event-opts="{{[['tap',[['pageScroll',['$event']]]]]}}" hidden="{{!(btnShow)}}" class="get-btn02" bindtap="__e">免费领取</button><block tt:if="{{popShow}}"><view data-event-opts="{{[['touchmove',[['e1',['$event']]]]]}}" class="pop-box" catchtouchmove="__e"><image class="pop-img" src="/static/index/pop.png" data-event-opts="{{[['tap',[['e2',['$event']]]]]}}" bindtap="__e"></image></view></block></view>