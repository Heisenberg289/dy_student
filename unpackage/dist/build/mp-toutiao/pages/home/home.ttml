<view class="content"><block tt:if="{{page_show}}"><block><view class="header"><view class="header-title"><text>今日课程</text><block tt:if="{{lesson_type!=1&&lesson_type!=4&&lesson_type!=2&&tableinfo.open_status!=1}}"><block><navigator class="header-title-to" url="{{'/pages/lesson_history/lesson_history?type='+tableinfo.member_type}}">已上课程</navigator></block></block></view><navigator class="three-day-vip" url="/pages/index/index" hover-class="none"><text class="text">{{"已有"+userinfo.num_now+"人领取"}}</text><button class="three-day-vip-btn" type="button">免费领取</button></navigator></view><form><view class="nav-box" hover-class="none" hover-stop-propagation="{{false}}"><view class="item"><view class="top"><button data-event-opts="{{[['tap',[['toChildVideo',['$event']]]]]}}" class="bbspx" bindtap="__e"></button><button data-event-opts="{{[['tap',[['toWrite',['$event']]]]]}}" class="lzxzs" bindtap="__e"></button><button data-event-opts="{{[['tap',[['toStrify',['$event']]]]]}}" class="xmjxz" bindtap="__e"></button></view></view></view></form><view class="job-list-box"><view class="job-list-head"><text class="job-list-title">作业广场</text><swiper class="job-head-right" autoplay circular="{{true}}" vertical="{{true}}" skip-hidden-item-layout="{{true}}" disable-touch><block tt:for="{{joblist.list}}" tt:for-item="item" tt:for-index="index" tt:key="index"><swiper-item data-event-opts="{{[['touchmove',[['moveHandle',['$event']]]]]}}" class="job-list-swiper-item" catchtouchmove="__e"><image class="job-list-swiper-img" src="{{imgsrc+'v2_0swiper-img.png'}}"></image><text class="job-list-swiper-name">{{item.show_name}}</text><text class="job-list-swiper-text">已提交作业</text></swiper-item></block></swiper></view><view class="job-list"><block tt:for="{{joblist.list}}" tt:for-item="item" tt:for-index="index" tt:key="index"><navigator class="job-list-item"><image class="job-item-img" src="{{item.job_img}}" lazy-load="{{true}}"></image><text class="job-item-text">{{item.txt}}</text><view class="job-item-user"><image class="job-item-user-img" src="{{item.user_head}}" lazy-load="{{true}}"></image><text class="job-item-user-name">{{item.name}}</text></view></navigator></block></view><block tt:if="{{joblist.more}}"><view class="job-list-tips">{{joblist.more_text}}</view></block></view></block></block><block tt:else><image class="loading-bg" src="/static/loading-bg.gif"></image></block></view>